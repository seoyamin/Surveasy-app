<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#FFFFFF"
    tools:context=".my.history.detail.MyViewHistoryDetailActivity">

    <androidx.appcompat.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:id="@+id/ToolbarHistoryDetail"
        android:theme="@style/ToolbarTheme"
        android:paddingTop="20dp"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@color/white"
        android:paddingBottom="10dp"
        app:titleTextColor="@color/black">

        <TextView
            android:paddingTop="5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/main_font_sb"
            android:textColor="@color/black"
            android:lineSpacingExtra="3.3sp"
            android:layout_gravity="center_horizontal"
            android:text="참여 상세 내역"
            android:textSize="20sp" />
    </androidx.appcompat.widget.Toolbar>


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/ToolbarHistoryDetail"

        android:id="@+id/historyDetailFragment"/>



</androidx.constraintlayout.widget.ConstraintLayout>