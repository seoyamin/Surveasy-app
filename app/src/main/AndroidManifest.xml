<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.surveasy">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name=".GlobalApplicationKakao"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Surveasy">
        <activity
            android:name=".list.SurveyListDetailResponseProof"
            android:exported="true" />
        <activity
            android:name=".my.MyViewNoticeListDetailActivity"
            android:exported="true" /> <!-- <activity -->
        <!-- android:name=".SplashActivity" -->
        <!-- android:exported="true" /> -->

        <activity
            android:name="com.kakao.sdk.auth.AuthCodeHandlerActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <!-- Redirect URI: "kakao{NATIVE_APP_KEY}://oauth" -->
                <data android:host="oauth"
                    android:scheme="kakaoc3dd0da30ec3cf75837fef9022ca48a4" />
            </intent-filter>
        </activity>


        <activity
            android:name=".MainActivity"
            android:exported="true" />
        <activity
            android:name=".list.SurveyListFirstSurveyActivity"
            android:exported="true" />
        <activity
            android:name=".list.SurveyListDetailResponseActivity"
            android:exported="true" />
        <activity
            android:name=".list.SurveyListDetailActivity"
            android:exported="true" />
        <activity
            android:name=".login.LoginActivity"
            android:exported="true"
            tools:ignore="DuplicateActivity" />
        <activity android:name=".login.RegisterActivity" /> <!-- <activity -->
        <!-- android:name=".MyViewFragment" -->
        <!-- android:exported="true" /> -->
        <activity
            android:name=".my.MyViewSettingActivity"
            android:exported="true" />
        <activity
            android:name=".my.MyViewSettingPushActivity"
            android:exported="true" />
        <activity
            android:name=".my.MyViewSettingWithdrawActivity"
            android:exported="true" />
        <activity
            android:name=".my.MyViewHistoryActivity"
            android:exported="true" />
        <activity
            android:name=".my.MyViewInfoActivity"
            android:exported="true" />
        <activity
            android:name=".my.MyViewContactActivity"
            android:exported="true" />
        <activity
            android:name=".my.MyViewInviteActivity"
            android:exported="true" />
        <activity
            android:name=".my.MyViewNoticeListActivity"
            android:exported="true" /> <!-- <activity -->
        <!-- android:name=".SurveyListActivity" -->
        <!-- android:exported="true" -->
        <!-- android:label="@string/title_activity_list" -->
        <!-- android:theme="@style/Theme.Surveasy.NoActionBar" /> -->
        <activity
            android:name=".SplashActivity"
            android:exported="true"
            android:theme="@style/Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service
            android:name=".MainActivity"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>